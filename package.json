{
  "name": "create-pr-on-install",
  "version": "1.0.0",
  "description": "A GitHub Extension built with Probot that helps you open a pull request when your app is installed.",
  "author": "Bex Warner <bexmwarner@gmail.com> (https://github.com/hiimbex/create-pr-on-installation)",
  "license": "ISC",
  "repository": "https://github.com/hiimbex/create-pr-on-install.git",
  "homepage": "https://github.com/hiimbex/create-pr-on-install",
  "bugs": "https://github.com/hiimbex/create-pr-on-install/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "dev": "nodemon --exec \"npm start\"",
    "start": "probot run ./index.js",
    "lint": "standard --fix",
    "test": "jest && standard",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "probot": "^6.2.0"
  },
  "devDependencies": {
    "jest": "^22.4.3",
    "nodemon": "^1.17.2",
    "smee-client": "^1.0.1",
    "standard": "^10.0.3"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "standard": {
    "env": [
      "jest"
    ]
  }
}
